# Environment variables for web client
# Copy this to .env.local and update with your values

# Signaling server URL
VITE_SIGNALING_URL=ws://localhost:8080/ws

# STUN server (use Google's public STUN for development)
VITE_STUN_URL=stun:stun.l.google.com:19302

# TURN server (optional, for NAT traversal)
VITE_TURN_URL=turn:localhost:3478
VITE_TURN_USERNAME=testuser
VITE_TURN_CREDENTIAL=testpassword
